class Solution {
public:
    int jump(vector<int>& nums) {
        int jumps=0,l=0,r=0;
        int n=nums.size();
        while(r<n-1){
            int furthest=0;
            for(int i=l;i<=r;i++){
                furthest=max(i+nums[i],furthest);
            }
            l=r+1;
            r=furthest;
            jumps=jumps+1;
        }
        return jumps;

        
    }
};
